{
  "name": "job-runner",
  "version": "1.0.0",
  "description": "Blank NPM package template with TypeScript + Jest support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "publish": "npm run build && npm publish",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "start": "npm run start:example1",
    "start:example1": "ts-node-dev src/examples/server.ts",
    "start:example2": "ts-node-dev src/examples/server-cancellable-and-custom-info.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denkan/npm-typescript-template.git"
  },
  "keywords": [
    "npm",
    "package",
    "template",
    "typescript",
    "jest"
  ],
  "author": "Dennis Nystr√∂m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/denkan/npm-typescript-template/issues"
  },
  "homepage": "https://github.com/denkan/npm-typescript-template#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "express": "^4.17.3",
    "jest": "^26.6.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "tiny-typed-emitter": "^2.1.0"
  }
}
